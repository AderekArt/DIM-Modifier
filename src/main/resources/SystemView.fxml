<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.cfogrady.dim.modifier.view.controller.SystemViewController">
   <children>
      <VBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <ImageView fx:id="backgroundsView" fitHeight="160.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true"/>
            <HBox spacing="10.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <Button fx:id="prevBackgroundButton" mnemonicParsing="false" text="Prev" />
                  <Button fx:id="nextBackgroundButton" mnemonicParsing="false" text="Next" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <VBox spacing="10.0">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="10.0">
               <children>
                  <VBox alignment="BOTTOM_LEFT" spacing="10.0">
                     <children>
                        <HBox alignment="CENTER">
                           <children>
                              <StackPane style="-fx-background-color: black;">
                                 <children>
                                    <ImageView fx:id="textSpritesView" pickOnBounds="true" preserveRatio="true"/>
                                 </children>
                              </StackPane>
                           </children>
                        </HBox>
                        <HBox spacing="10.0">
                           <children>
                              <Button fx:id="prevTextSpriteButton" mnemonicParsing="false" text="Prev" />
                              <Button fx:id="nextTextSpriteButton" mnemonicParsing="false" text="Next" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="BOTTOM_LEFT" spacing="10.0">
                     <children>
                        <HBox alignment="CENTER">
                           <children>
                              <StackPane style="-fx-background-color: black;">
                                 <children>
                                    <ImageView fx:id="stageSpritesView" pickOnBounds="true" preserveRatio="true"/>
                                 </children>
                              </StackPane>
                           </children>
                        </HBox>
                        <HBox spacing="10.0">
                           <children>
                              <Button fx:id="prevStageButton" mnemonicParsing="false" text="Prev" />
                              <Button fx:id="nextStageButton" mnemonicParsing="false" text="Next" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="BOTTOM_LEFT" spacing="10.0">
                     <children>
                        <HBox alignment="CENTER">
                           <children>
                              <StackPane style="-fx-background-color: black;">
                                 <children>
                                    <ImageView fx:id="attributeSpritesView" pickOnBounds="true" preserveRatio="true"/>
                                 </children>
                              </StackPane>
                           </children>
                        </HBox>
                        <HBox spacing="10.0">
                           <children>
                              <Button fx:id="prevAttributeButton" mnemonicParsing="false" text="Prev" />
                              <Button fx:id="nextAttributeButton" mnemonicParsing="false" text="Next" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER">
                     <children>
                        <StackPane style="-fx-background-color: black;">
                           <children>
                              <ImageView fx:id="iconSpriteView" pickOnBounds="true" preserveRatio="true"/>
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="BOTTOM_LEFT" spacing="10.0">
               <children>
                  <VBox alignment="TOP_CENTER" spacing="10.0">
                     <children>
                        <ImageView fx:id="eggSpritesView" pickOnBounds="true" preserveRatio="true"/>
                        <HBox spacing="10.0">
                           <children>
                              <Button fx:id="prevEggButton" mnemonicParsing="false" text="Prev" />
                              <Button fx:id="nextEggButton" mnemonicParsing="false" text="Next" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="BOTTOM_LEFT" spacing="10.0">
                     <children>
                        <HBox alignment="CENTER">
                           <children>
                              <StackPane style="-fx-background-color: black;">
                                 <children>
                                    <ImageView fx:id="hitSpritesView" pickOnBounds="true" preserveRatio="true"/>
                                 </children>
                              </StackPane>
                           </children>
                        </HBox>
                        <HBox spacing="10.0">
                           <children>
                              <Button fx:id="prevHitButton" mnemonicParsing="false" text="Prev" />
                              <Button fx:id="nextHitButton" mnemonicParsing="false" text="Next" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="BOTTOM_LEFT" spacing="10.0">
                     <children>
                        <HBox alignment="CENTER">
                           <children>
                              <StackPane>
                                 <children>
                                    <ImageView fx:id="smallAttackSpritesView" pickOnBounds="true" preserveRatio="true"/>
                                 </children>
                              </StackPane>
                           </children>
                        </HBox>
                        <HBox spacing="10.0">
                           <children>
                              <Button fx:id="prevSmallAttackButton" mnemonicParsing="false" text="Prev" />
                              <Button fx:id="nextSmallAttackButton" mnemonicParsing="false" text="Next" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox alignment="BOTTOM_LEFT" spacing="10.0">
                     <children>
                        <HBox alignment="CENTER">
                           <children>
                              <StackPane>
                                 <children>
                                    <ImageView fx:id="bigAttackSpritesView" pickOnBounds="true" preserveRatio="true"/>
                                 </children>
                              </StackPane>
                           </children>
                        </HBox>
                        <HBox spacing="10.0">
                           <children>
                              <Button fx:id="prevBigAttackButton" mnemonicParsing="false" text="Prev" />
                              <Button fx:id="nextBigAttackButton" mnemonicParsing="false" text="Next" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</HBox>
